{
  "name": "open-xml-powertools-js",
  "version": "0.1.1",
  "license": "MIT",
  "type": "module",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "files": [
    "src/",
    "README.md",
    "LICENSE",
    "NOTICE.md",
    "spec.md"
  ],
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
